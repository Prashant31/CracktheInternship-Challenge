Remove Duplicates from Sorted Array
 int removeDuplicates(vector<int>& nums) {
        int z=1;
        int j=1;
        for(int i=0;j<nums.size();){
            if(nums[i]==nums[j]){
                j++;
            }
            else{
                nums[z]=nums[j];
                i++;
                z++;
            }
        }
        return z;
    }
